{
    "name": "Operaatio RTX 2021",
    "description": "Otetaan helpot pois ðŸ˜Ž",
    "version": "1.3.0",
    "manifest_version": 2,
    "icons": {
        "16": "icon3.png",
        "48": "icon2.png",
        "128": "icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "clipboardWrite",
        "storage"
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    }
  }